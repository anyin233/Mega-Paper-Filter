{
  "name": "PubMed Papers",
  "description": "Configuration for PubMed/medical paper datasets",
  "csv_columns": {
    "title": "Title",
    "author": "Authors", 
    "summary": "Abstract",
    "keywords": "Keywords",
    "key": "PMID",
    "year": "Publication Date",
    "doi": "DOI",
    "url": "URL"
  },
  "tfidf_params": {
    "max_features": 2000,
    "stop_words": "english",
    "ngram_range": [1, 3],
    "min_df": 2,
    "max_df": 0.9
  },
  "clustering_params": {
    "max_k": 25,
    "random_state": 42,
    "n_init": 10
  },
  "pca_params": {
    "n_components": 2,
    "random_state": 42
  }
}